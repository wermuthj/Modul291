cts']),\n  methods: {\n    addToCart(product) {\n      this.$store.commit('addToCart', product);\n    },\n    removeFromCart(product) {\n      this.$store.commit('removeFromCart', product);\n    }\n  },\n  created() {\n    this.$store.dispatch(\"loadProducts\")\n  }\n}\n</script>\n\n"]}]}